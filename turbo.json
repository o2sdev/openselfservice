{
    "$schema": "https://turbo.build/schema.json",
    "ui": "stream",
    "concurrency": "5",
    "tasks": {
        "dev": {
            "cache": false,
            "outputLogs": "full",
            "persistent": true,
            "inputs": ["$TURBO_DEFAULT$", ".env*"]
        },
        "build": {
            "cache": true,
            "outputLogs": "new-only",
            "dependsOn": ["^build"],
            "inputs": ["$TURBO_DEFAULT$", ".env*"],
            "outputs": ["dist/**", "build/**", ".next/**", "!.next/cache/**"]
        },
        "lint": {
            "cache": true,
            "outputLogs": "new-only",
            "dependsOn": ["^lint"]
        },
        "test": {
            "cache": true,
            "outputLogs": "new-only",
            "dependsOn": ["^test"]
        },
        "format": {
            "cache": true,
            "outputLogs": "new-only",
            "dependsOn": ["^format"]
        },
        "start": {
            "cache": false,
            "outputLogs": "full",
            "persistent": true
        }
    }
}
