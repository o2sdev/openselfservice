{
    "name": "@o2s/auth.mocked",
    "version": "0.0.1",
    "private": false,
    "license": "MIT",
    "exports": {
        ".": "./dist/src/index.js"
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "prepare": "npx prisma generate && dotenv -e .env.local -- prisma migrate deploy && dotenv -e .env.local -- prisma db seed",
        "dev": "tsc && (concurrently \"tsc -w\" \"tsc-alias -w\")",
        "build": "dotenv -e .env.local -- prisma migrate deploy && tsc && tsc-alias",
        "lint": "tsc --noEmit && eslint . --ignore-pattern .eslintrc.js --max-warnings 0",
        "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,scss,json}\"",
        "seed": "dotenv -e .env.local -- tsx prisma/seed.ts"
    },
    "prisma": {
        "seed": "tsx prisma/seed.ts"
    },
    "dependencies": {
        "@auth/prisma-adapter": "^2.9.1",
        "@prisma/client": "^6.8.2",
        "bcryptjs": "^3.0.2",
        "dotenv-cli": "^8.0.0",
        "jsonwebtoken": "^9.0.2",
        "prisma": "^6.8.2"
    },
    "devDependencies": {
        "@types/jsonwebtoken": "^9.0.9"
    },
    "peerDependencies": {
        "next-auth": "^5.0.0-beta.27",
        "zod": "^3.25.4"
    }
}
