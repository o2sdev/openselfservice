        "@o2s/lint-staged-config": "*",
        "@o2s/prettier-config": "*",
        "@o2s/typescript-config": "*",
        "@types/cli-progress": "^3.11.6",
        "eslint": "^9.39.2",
        "prettier": "^3.8.1",
        "typescript": "^5.9.3"
