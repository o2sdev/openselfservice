{
    "name": "create-o2s-app",
    "version": "1.2.1",
    "private": false,
    "license": "MIT",
    "description": "CLI tool to scaffold new O2S applications with Next.js and NestJS setup.",
    "keywords": [
        "o2s",
        "cli",
        "scaffold",
        "create-app",
        "nextjs",
        "nestjs"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/o2sdev/openselfservice.git",
        "directory": "packages/cli/create-o2s-app"
    },
    "homepage": "https://github.com/o2sdev/openselfservice/tree/main/packages/cli/create-o2s-app#readme",
    "bugs": {
        "url": "https://github.com/o2sdev/openselfservice/issues"
    },
    "main": "./dist/index.js",
    "bin": {
        "create-o2s-app": "./dist/index.js"
    },
    "scripts": {
        "create-o2s-app": "ts-node ./src/index.ts",
        "build": "tsc --preserveWatchOutput && tsc-alias",
        "lint": "tsc --noEmit && eslint . --max-warnings=0",
        "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,scss,json}\""
    },
    "dependencies": {
        "@o2s/telemetry": "^1.2.0",
        "@types/prompts": "^2.4.9",
        "cli-progress": "^3.12.0",
        "commander": "^14.0.3",
        "prompts": "^2.4.2",
        "simple-git": "^3.30.0",
        "ts-node": "^10.9.2"
    },
    "devDependencies": {
        "@o2s/eslint-config": "*",
        "@o2s/lint-staged-config": "*",
        "@o2s/prettier-config": "*",
        "@o2s/typescript-config": "*",
        "@types/cli-progress": "^3.11.6",
        "eslint": "^9.39.3",
        "prettier": "^3.8.1",
        "typescript": "^5.9.3"
    }
}
