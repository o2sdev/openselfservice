{
    "name": "@o2s/framework",
    "version": "1.13.0",
    "private": false,
    "license": "MIT",
    "exports": {
        "./modules": "./dist/src/index.js",
        "./sdk": "./dist/src/sdk.js"
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsc --preserveWatchOutput && tsc-alias",
        "lint": "tsc --noEmit && eslint . --ignore-pattern .eslintrc.js --max-warnings=0",
        "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,scss,json}\"",
        "generate:component": "turbo gen core-component"
    },
    "dependencies": {
        "@o2s/utils.logger": "*",
        "logform": "2.7.0",
        "ofetch": "^1.5.1",
        "qs": "^6.14.0",
        "winston": "^3.18.3"
    },
    "devDependencies": {
        "@o2s/eslint-config": "*",
        "@o2s/lint-staged-config": "*",
        "@o2s/prettier-config": "*",
        "@o2s/typescript-config": "*",
        "@types/express": "^5.0.6",
        "@turbo/gen": "^2.6.2",
        "@types/qs": "^6.14.0",
        "concurrently": "^9.2.1",
        "eslint": "^9.39.1",
        "jsonwebtoken": "^9.0.2",
        "prettier": "^3.7.4",
        "tsc-alias": "^1.8.16",
        "typescript": "^5.9.3"
    },
    "peerDependencies": {
        "@nestjs/axios": "^4.0.1",
        "@nestjs/common": "^11.0.16",
        "@nestjs/config": "^4.0.2",
        "@nestjs/core": "^11",
        "rxjs": "^7"
    }
}
