        "dotenv-cli": "^11.0.0",
        "@o2s/eslint-config": "*",
        "@o2s/lint-staged-config": "*",
        "@o2s/prettier-config": "*",
        "@o2s/typescript-config": "*",
        "concurrently": "^9.1.2",
        "eslint": "^9.27.0",
        "prettier": "^3.5.3",
