{
    "name": "@o2s/blocks.notification-list",
    "version": "1.1.0",
    "private": false,
    "license": "MIT",
    "description": "A block displaying and managing notifications.",
    "exports": {
        "./api-harmonization": "./dist/api-harmonization/api-harmonization/index.js",
        "./frontend": "./dist/frontend/frontend/index.js",
        "./sdk": "./dist/sdk/sdk/index.js",
        "./client": "./dist/api-harmonization/api-harmonization/notification-list.client.js"
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "dev": "tsc --build tsconfig.json  && (concurrently \"tsc --build tsconfig.json -w\" \"tsc-alias -w\")",
        "build": "tsc --build tsconfig.json && tsc-alias",
        "lint": "tsc --noEmit && eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,scss,json}\""
    },
    "dependencies": {
        "@o2s/framework": "*",
        "@o2s/utils.logger": "*",
        "@o2s/ui": "*",
        "@o2s/utils.api-harmonization": "*",
        "@o2s/utils.frontend": "*",
        "@o2s/configs.integrations": "*"
    },
    "devDependencies": {
        "dotenv-cli": "^10.0.0",
        "@o2s/eslint-config": "*",
        "@o2s/prettier-config": "*",
        "@o2s/typescript-config": "*",
        "concurrently": "^9.2.1",
        "eslint": "^9.38.0",
        "prettier": "^3.6.2",
        "tsc-alias": "^1.8.16",
        "typescript": "^5.9.3"
    },
    "peerDependencies": {
        "@types/react": "^19",
        "@types/react-dom": "^19",
        "react": "^19",
        "react-dom": "^19",
        "tailwindcss": "^4",
        "@nestjs/axios": "^4.0.1",
        "@nestjs/common": "^11.0.16",
        "@nestjs/config": "^4.0.2",
        "@nestjs/core": "^11",
        "rxjs": "^7",
        "next": "^15.4.7",
        "next-intl": "^4.1.0"
    }
}
