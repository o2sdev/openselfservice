        "eslint-config-turbo": "^2.5.2",
        "eslint-plugin-only-warn": "^1.1.0",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-turbo": "^2.5.2",
        "globals": "^16.0.0",
