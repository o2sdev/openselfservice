{
    "$schema": "https://json.schemastore.org/tsconfig",
    "extends": "./base.json",
    "compilerOptions": {
        "plugins": [
            {
                "name": "next"
            }
        ],
        "types": [
            "vite/client"
        ],
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "allowJs": true,
        "jsx": "preserve",
        "noEmit": true
    }
}