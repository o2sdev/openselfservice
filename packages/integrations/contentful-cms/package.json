{
    "name": "@o2s/integrations.contentful-cms",
    "version": "0.3.0",
    "private": false,
    "license": "MIT",
    "exports": {
        "./integration": "./dist/src/integration.js",
        "./live-preview": "./dist/src/modules/cms/live-preview/index.js"
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsc --preserveWatchOutput && tsc-alias",
        "lint": "tsc --noEmit && eslint . --max-warnings 0",
        "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,scss,json}\"",
        "generate": "graphql-codegen && prettier --write \"generated/**/*.{ts,tsx}\""
    },
    "dependencies": {
        "@contentful/live-preview": "^4.6.52",
        "@o2s/framework": "*",
        "@o2s/utils.logger": "*",
        "contentful": "^11.0.0",
        "flatted": "^3.3.3",
        "graphql": "16.12.0",
        "graphql-request": "7.3.4",
        "graphql-tag": "2.12.6"
    },
    "devDependencies": {
        "@graphql-codegen/add": "^6.0.0",
        "@graphql-codegen/cli": "^6.1.0",
        "@graphql-codegen/introspection": "^5.0.0",
        "@graphql-codegen/typescript": "^5.0.6",
        "@graphql-codegen/typescript-graphql-request": "^6.3.0",
        "@graphql-codegen/typescript-operations": "^5.0.6",
        "@graphql-codegen/typescript-resolvers": "^5.1.4",
        "@o2s/eslint-config": "*",
        "@o2s/prettier-config": "*",
        "@o2s/typescript-config": "*",
        "concurrently": "^9.2.1",
        "eslint": "^9.39.1",
        "prettier": "^3.6.2",
        "tsc-alias": "^1.8.16",
        "typescript": "^5.9.3"
    },
    "peerDependencies": {
        "@nestjs/axios": "^4.0.1",
        "@nestjs/common": "^11.0.16",
        "@nestjs/config": "^4.0.2",
        "@nestjs/core": "^11",
        "rxjs": "^7"
    }
}
