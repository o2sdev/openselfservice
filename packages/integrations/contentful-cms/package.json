{
    "name": "@o2s/integrations.contentful-cms",
    "version": "0.6.0",
    "private": false,
    "license": "MIT",
    "description": "Contentful CMS integration for O2S, providing content management functionality via GraphQL.",
    "keywords": [
        "o2s",
        "integration",
        "contentful",
        "cms",
        "content"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/o2sdev/openselfservice.git",
        "directory": "packages/integrations/contentful-cms"
    },
    "homepage": "https://github.com/o2sdev/openselfservice/tree/main/packages/integrations/contentful-cms#readme",
    "bugs": {
        "url": "https://github.com/o2sdev/openselfservice/issues"
    },
    "exports": {
        "./integration": "./dist/src/integration.js",
        "./live-preview": "./dist/src/modules/cms/live-preview/index.js"
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "test": "vitest run",
        "build": "tsc --preserveWatchOutput && tsc-alias",
        "lint": "tsc --noEmit && eslint . --max-warnings=0",
        "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,scss,json}\"",
        "generate": "graphql-codegen && prettier --write \"generated/**/*.{ts,tsx}\""
    },
    "dependencies": {
        "@contentful/live-preview": "^4.9.4",
        "@o2s/framework": "*",
        "@o2s/utils.logger": "*",
        "contentful": "^11.10.3",
        "flatted": "^3.3.3",
        "graphql": "16.12.0",
        "graphql-request": "7.4.0",
        "graphql-tag": "2.12.6"
    },
    "devDependencies": {
        "@graphql-codegen/add": "^6.0.0",
        "@graphql-codegen/cli": "^6.1.1",
        "@graphql-codegen/introspection": "^5.0.0",
        "@graphql-codegen/typescript": "^5.0.7",
        "@graphql-codegen/typescript-graphql-request": "^6.4.0",
        "@graphql-codegen/typescript-operations": "^5.0.7",
        "@graphql-codegen/typescript-resolvers": "^5.1.5",
        "@o2s/eslint-config": "*",
        "@o2s/lint-staged-config": "*",
        "@o2s/prettier-config": "*",
        "@o2s/typescript-config": "*",
        "@o2s/vitest-config": "*",
        "concurrently": "^9.2.1",
        "eslint": "^9.39.2",
        "prettier": "^3.8.1",
        "tsc-alias": "^1.8.16",
        "typescript": "^5.9.3",
        "vitest": "^4.0.18"
    },
    "peerDependencies": {
        "@nestjs/axios": "^4.0.1",
        "@nestjs/common": "^11.0.16",
        "@nestjs/config": "^4.0.2",
        "@nestjs/core": "^11",
        "@types/react": "^19",
        "@types/react-dom": "^19",
        "react": "^19",
        "react-dom": "^19",
        "rxjs": "^7"
    }
}
