                "@o2s/utils.logger#build",
                "@o2s/framework#build"
            ],
            "env": [
                "CF_TOKEN",
                "CF_PREVIEW_TOKEN",
                "CF_SPACE_ID",
                "CF_ENV",
                "DEFAULT_LOCALE"
