{
  "name": "@o2s/integrations.zendesk",
  "version": "1.0.0",
  "private": false,
  "license": "MIT",
  "exports": {
    "./integration": "./dist/src/integration.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "npm run fetch-oas && npm run generate-types",
    "build": "tsc --preserveWatchOutput && tsc-alias",
    "lint": "tsc --noEmit && eslint ./src --max-warnings 0",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,scss,json}\"",
    "fetch-oas": "tsx scripts/oas/fetch-zendesk-oas.ts",
    "generate-types": "tsx scripts/oas/generate-zendesk-types.ts"
  },
  "dependencies": {
    "@nestjs/axios": "^3.1.3",
    "@nestjs/common": "^10.4.20",
    "@o2s/framework": "*",
    "@o2s/utils.logger": "*",
    "axios": "^1.13.2",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@hey-api/openapi-ts": "^0.88.0",
    "@o2s/eslint-config": "*",
    "@o2s/prettier-config": "*",
    "@o2s/typescript-config": "*",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.1",
    "prettier": "^3.6.2",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
