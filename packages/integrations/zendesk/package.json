{
  "name": "@o2s/integrations.zendesk",
  "version": "3.0.0",
  "private": false,
  "license": "MIT",
  "description": "Zendesk integration for O2S, providing ticket management and support functionality.",
  "keywords": [
    "o2s",
    "integration",
    "zendesk",
    "support",
    "tickets"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/o2sdev/openselfservice.git",
    "directory": "packages/integrations/zendesk"
  },
  "homepage": "https://github.com/o2sdev/openselfservice/tree/main/packages/integrations/zendesk#readme",
  "bugs": {
    "url": "https://github.com/o2sdev/openselfservice/issues"
  },
  "exports": {
    "./integration": "./dist/src/integration.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "npm run fetch-oas && npm run generate-types",
    "test": "vitest run",
    "build": "tsc --preserveWatchOutput && tsc-alias",
    "lint": "tsc --noEmit && eslint ./src --max-warnings=0",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,scss,json}\"",
    "fetch-oas": "tsx scripts/oas/fetch-zendesk-oas.ts",
    "generate-types": "tsx scripts/oas/generate-zendesk-types.mts"
  },
  "dependencies": {
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "^11.1.14",
    "@o2s/framework": "*",
    "@o2s/utils.logger": "*",
    "axios": "^1.13.5",
    "html-to-text": "^9.0.5",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@hey-api/openapi-ts": "^0.92.4",
    "@o2s/eslint-config": "*",
    "@o2s/vitest-config": "*",
    "@o2s/prettier-config": "*",
    "@o2s/typescript-config": "*",
    "@types/html-to-text": "^9.0.4",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.3",
    "prettier": "^3.8.1",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
