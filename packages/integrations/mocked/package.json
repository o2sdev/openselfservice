{
    "name": "@o2s/integrations.mocked",
    "version": "1.18.0",
    "private": false,
    "license": "MIT",
    "description": "Mocked integration for O2S development and testing, providing in-memory data with Prisma.",
    "keywords": [
        "o2s",
        "integration",
        "mocked",
        "mock",
        "development"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/o2sdev/openselfservice.git",
        "directory": "packages/integrations/mocked"
    },
    "homepage": "https://github.com/o2sdev/openselfservice/tree/main/packages/integrations/mocked#readme",
    "bugs": {
        "url": "https://github.com/o2sdev/openselfservice/issues"
    },
    "exports": {
        "./integration": "./dist/integration.js",
        "./modules": "./dist/modules/index.js",
        "./sdk": "./dist/sdk/index.js",
        "./auth": "./dist/auth/index.js",
        "./auth.updateOrganization": "./dist/auth/auth.updateOrganization.js",
        "./live-preview": "./dist/modules/cms/live-preview/index.js"
    },
    "files": [
        "dist",
        "prisma",
        ".env.local"
    ],
    "scripts": {
        "prepare": "npx prisma generate && dotenv -e .env.local -- prisma migrate deploy && dotenv -e .env.local -- prisma db seed",
        "postinstall": "npx prisma generate",
        "build": "dotenv -e .env.local -- prisma migrate deploy && tsc --preserveWatchOutput && tsc-alias",
        "postbuild": "shx cp -r src/modules/invoices/resources dist/modules/invoices/",
        "test": "vitest run",
        "lint": "tsc --noEmit && eslint . --max-warnings=0",
        "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,scss,json}\"",
        "seed": "dotenv -e .env.local -- tsx prisma/seed.ts"
    },
    "prisma": {
        "schema": "./prisma/schema.prisma",
        "seed": "tsx prisma/seed.ts"
    },
    "dependencies": {
        "@auth/prisma-adapter": "^2.11.1",
        "@o2s/framework": "*",
        "@o2s/utils.logger": "*",
        "@prisma/client": "^6.19.2",
        "bcryptjs": "^3.0.3",
        "dotenv-cli": "^11.0.0",
        "jsonwebtoken": "^9.0.3",
        "prisma": "^6.19.2"
    },
    "devDependencies": {
        "@o2s/eslint-config": "*",
        "@o2s/lint-staged-config": "*",
        "@o2s/prettier-config": "*",
        "@o2s/typescript-config": "*",
        "@o2s/vitest-config": "*",
        "@o2s/telemetry": "*",
        "@types/jsonwebtoken": "^9.0.10",
        "concurrently": "^9.2.1",
        "eslint": "^9.39.2",
        "express": "5.2.1",
        "prettier": "^3.8.1",
        "shx": "^0.4.0",
        "tsc-alias": "^1.8.16",
        "typescript": "^5.9.3",
        "vitest": "^4.0.18"
    },
    "peerDependencies": {
        "@nestjs/axios": "^4.0.1",
        "@nestjs/common": "^11.0.16",
        "@nestjs/config": "^4.0.2",
        "@nestjs/core": "^11",
        "@types/react": "^19",
        "@types/react-dom": "^19",
        "next-auth": "^5.0.0-beta.30",
        "react": "^19",
        "react-dom": "^19",
        "rxjs": "^7",
        "zod": "^3.25.4 || ^4.0.0"
    },
    "o2sTemplate": ["ssp"]
}
