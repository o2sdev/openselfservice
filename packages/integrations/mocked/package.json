{
    "name": "@o2s/integrations.mocked",
    "version": "0.14.0",
    "private": false,
    "license": "MIT",
    "exports": {
        "./integration": "./dist/integration.js",
        "./sdk": "./dist/sdk/index.js"
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "dev": "tsc && (concurrently \"tsc -w\" \"tsc-alias -w\")",
        "build": "tsc && tsc-alias",
        "postbuild": "cp -r src/modules/invoices/resources dist/modules/invoices/",
        "lint": "tsc --noEmit && eslint . --max-warnings 0",
        "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,scss,json}\""
    },
    "dependencies": {
        "@nestjs/axios": "^3.1.2",
        "@nestjs/common": "^10.0.0",
        "@nestjs/config": "^3.3.0",
        "@nestjs/core": "^10.0.0",
        "@o2s/framework": "*",
        "@o2s/utils.logger": "*",
        "rxjs": "^7.8.1"
    },
    "devDependencies": {
        "@o2s/eslint-config": "*",
        "@o2s/prettier-config": "*",
        "@o2s/typescript-config": "*",
        "concurrently": "^9.1.2",
        "eslint": "^8.57.0",
        "prettier": "^3.5.3",
        "tsc-alias": "^1.8.11",
        "typescript": "^5.8.2"
    }
}
