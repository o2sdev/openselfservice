        "react-dom": "19.2.0",
        "react-string-replace": "^1.1.1",
        "string-template": "^1.0.0",
        "yup": "^1.7.1",
        "zod": "^4.1.12"
    },
    "devDependencies": {
        "@next/bundle-analyzer": "16.0.5",
        "@o2s/eslint-config": "*",
        "@o2s/prettier-config": "*",
        "@o2s/typescript-config": "*",
        "@svgr/webpack": "^8.1.0",
        "@trivago/prettier-plugin-sort-imports": "^6.0.0",
