{
    "name": "@o2s/frontend",
    "version": "0.25.1",
    "private": true,
    "scripts": {
        "prepare": "npx prisma generate && dotenv -e .env.local -- prisma migrate deploy && dotenv -e .env.local -- prisma db seed",
        "dev": "next dev --turbopack",
        "dev:https": "next dev --turbopack --experimental-https",
        "build": "dotenv -e .env.local -- prisma migrate deploy && next build",
        "start": "next start",
        "lint": "tsc --noEmit && next lint",
        "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,scss,json}\"",
        "generate:component": "turbo gen web-component",
        "seed": "dotenv -e .env.local -- tsx prisma/seed.ts"
    },
    "prisma": {
        "seed": "tsx prisma/seed.ts"
    },
    "dependencies": {
        "@auth/prisma-adapter": "^2.9.1",
        "@o2s/api-harmonization": "*",
        "@o2s/framework": "*",
        "@o2s/ui": "*",
        "@prisma/client": "^6.8.2",
        "bcryptjs": "^3.0.2",
        "class-variance-authority": "^0.7.1",
        "date-fns": "^4.1.0",
        "dotenv-cli": "^8.0.0",
        "formik": "^2.4.6",
        "jsonwebtoken": "^9.0.2",
        "markdown-to-jsx": "^7.7.6",
        "next": "^15.3.2",
        "next-auth": "^5.0.0-beta.27",
        "next-compose-plugins": "^2.2.1",
        "next-intl": "^4.1.0",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "react-indiana-drag-scroll": "^2.2.1",
        "react-string-replace": "^1.1.1",
        "string-template": "^1.0.0",
        "survey-core": "^2.0.9",
        "survey-react-ui": "^2.0.9",
        "yup": "^1.6.1",
        "zod": "^3.25.4"
    },
    "devDependencies": {
        "@next/bundle-analyzer": "^15.3.2",
        "@o2s/eslint-config": "*",
        "@o2s/prettier-config": "*",
        "@o2s/typescript-config": "*",
        "@svgr/webpack": "^8.1.0",
        "@trivago/prettier-plugin-sort-imports": "^5.2.2",
        "@types/bcryptjs": "^3.0.0",
        "@types/jsonwebtoken": "^9.0.9",
        "@types/node": "^22.15.19",
        "@types/react": "^19.1.4",
        "@types/react-dom": "^19.1.5",
        "dotenv": "^16.5.0",
        "eslint": "^9",
        "eslint-config-next": "^15.3.2",
        "postcss": "^8.5.3",
        "prettier": "^3.5.3",
        "prisma": "^6.8.2",
        "sass": "^1.89.0",
        "tailwindcss": "^4.1.7",
        "tsx": "^4.19.4",
        "typescript": "^5.8.3"
    }
}
