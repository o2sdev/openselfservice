{
    "name": "@o2s/frontend",
    "version": "0.20.0",
    "private": true,
    "scripts": {
        "prepare": "npx prisma generate && dotenv -e .env.local -- prisma migrate deploy && dotenv -e .env.local -- prisma db seed",
        "dev": "next dev --turbopack",
        "build": "dotenv -e .env.local -- prisma migrate deploy && next build",
        "start": "next start",
        "lint": "tsc --noEmit && next lint",
        "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,scss,json}\"",
        "generate:component": "turbo gen web-component",
        "seed": "dotenv -e .env.local -- tsx prisma/seed.ts"
    },
    "prisma": {
        "seed": "tsx prisma/seed.ts"
    },
    "dependencies": {
        "@auth/prisma-adapter": "^2.8.0",
        "@o2s/api-harmonization": "*",
        "@o2s/ui": "*",
        "@prisma/client": "^6.5.0",
        "bcryptjs": "^2.4.3",
        "class-variance-authority": "^0.7.1",
        "dotenv-cli": "^8.0.0",
        "formik": "^2.4.6",
        "jsonwebtoken": "^9.0.2",
        "markdown-to-jsx": "^7.7.4",
        "next": "15.2.4",
        "next-auth": "^5.0.0-beta.25",
        "next-compose-plugins": "^2.2.1",
        "next-intl": "^3.26.1",
        "next-pwa": "^5.6.0",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "react-string-replace": "^1.1.1",
        "string-template": "^1.0.0",
        "yup": "^1.6.1",
        "zod": "^3.24.2"
    },
    "devDependencies": {
        "@o2s/eslint-config": "*",
        "@o2s/prettier-config": "*",
        "@o2s/typescript-config": "*",
        "@next/bundle-analyzer": "15.2.4",
        "@svgr/webpack": "^8.1.0",
        "@trivago/prettier-plugin-sort-imports": "^4.3.0",
        "@types/bcryptjs": "^2.4.6",
        "@types/jsonwebtoken": "^9.0.9",
        "@types/next-pwa": "^5.6.9",
        "@types/node": "^20",
        "@types/react": "19.0.0",
        "@types/react-dom": "19.0.0",
        "dotenv": "^16.4.7",
        "eslint": "^8",
        "eslint-config-next": "15.2.4",
        "postcss": "^8",
        "prettier": "^3.5.3",
        "prisma": "^6.5.0",
        "sass": "^1.85.1",
        "tailwindcss": "^3.4.1",
        "tsx": "^4.19.3",
        "typescript": "^5"
    }
}
