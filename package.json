{
    "name": "o2s",
    "private": true,
    "scripts": {
        "prepare": "husky",
        "dev": "turbo dev --no-cache",
        "build": "turbo build",
        "start": "turbo start",
        "format": "turbo format",
        "lint": "turbo lint",
        "test": "turbo test &  vitest --project=storybook",
        "generate": "turbo gen",
        "eject-block": "ts-node ./scripts/cli.ts eject-block",
        "docs": "turbo start --filter=@o2s/docs",
        "changeset": "changeset",
        "version-packages": "changeset version",
        "prerelease": "turbo run build",
        "release": "changeset publish",
        "update-deps": "npx npm-check-updates --format group --workspaces --interactive",
        "find-missing-deps": "turbo boundaries",
        "storybook": "storybook dev -p 6006",
        "prebuild-storybook": "turbo build -F=!{./apps/*} -F=!{./packages/cli/*}",
        "build-storybook": "storybook build",
        "test:storybook": "vitest --project=storybook",
        "clean": "npm exec --workspaces -- npx rimraf node_modules dist .next .turbo && npx rimraf node_modules dist .next .turbo"
    },
    "engines": {
        "node": ">=22"
    },
    "packageManager": "npm@11.6.3",
    "workspaces": [
        "apps/*",
        "packages/*",
        "packages/configs/*",
        "packages/integrations/*",
        "packages/utils/*",
        "packages/cli/*",
        "packages/blocks/*",
        "packages/modules/*"
    ],
    "devDependencies": {
        "@commitlint/cli": "^20.1.0",
        "@commitlint/config-conventional": "^20.0.0",
        "@storybook/addon-a11y": "^10.0.8",
        "@storybook/addon-docs": "^10.0.8",
        "@storybook/addon-themes": "^10.0.8",
        "@storybook/addon-vitest": "^10.0.8",
        "@storybook/nextjs-vite": "^10.0.8",
        "@swc/core": "^1.15.3",
        "@turbo/gen": "^2.6.1",
        "@types/cli-progress": "^3.11.6",
        "@types/fs-extra": "^11.0.4",
        "@types/prompts": "^2.4.9",
        "@types/unzipper": "^0.10.11",
        "@vitejs/plugin-react": "^5.1.1",
        "@vitest/browser-playwright": "^4.0.14",
        "@vitest/coverage-v8": "^4.0.14",
        "dotenv": "^17.2.3",
        "fs-extra": "^11.3.2",
        "husky": "^9.1.7",
        "lint-staged": "^16.2.7",
        "playwright": "^1.56.1",
        "prettier": "^3.6.2",
        "simple-git": "^3.30.0",
        "storybook": "^10.0.8",
        "syncpack": "^14.0.0-alpha.27",
        "turbo": "^2.6.1",
        "typescript": "5.9.3",
        "unplugin-swc": "^1.5.9",
        "vite": "^7.2.4",
        "vite-plugin-svgr": "^4.5.0",
        "vite-tsconfig-paths": "^5.1.4",
        "vitest": "^4.0.14"
    },
    "dependencies": {
        "@changesets/cli": "^2.29.7",
        "@types/prompts": "^2.4.9",
        "cli-progress": "^3.12.0",
        "commander": "^14.0.2",
        "prompts": "^2.4.2",
        "unzipper": "^0.12.3"
    }
}
