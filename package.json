{
    "name": "o2s",
    "private": true,
    "scripts": {
        "prepare": "husky",
        "dev": "turbo dev",
        "build": "turbo build",
        "start": "turbo start",
        "format": "turbo format",
        "lint": "turbo lint",
        "test": "turbo test",
        "generate": "turbo gen",
        "docs": "turbo start --filter=@o2s/docs",
        "changeset": "changeset",
        "version-packages": "changeset version",
        "prerelease": "turbo run build",
        "release": "changeset publish"
    },
    "engines": {
        "node": ">=18"
    },
    "packageManager": "npm@11.3.0",
    "workspaces": [
        "apps/*",
        "packages/*",
        "packages/configs/*",
        "packages/integrations/*",
        "packages/utils/*"
    ],
    "devDependencies": {
        "husky": "^9.1.7",
        "lint-staged": "^15.5.0",
        "prettier": "^3.5.3",
        "turbo": "^2.5.0",
        "typescript": "5.8.3",
        "@types/react": "^19.1.0",
        "@types/react-dom": "^19.1.2"
    },
    "dependencies": {
        "@changesets/cli": "^2.28.1",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "@nestjs/common": "^11.0.17",
        "@nestjs/core": "^11.0.17",
        "rxjs": "^7.8.1"
    }
}
