{{#if (eq module 'cms')}}
    import { CMS } from '@o2s/framework/modules';
{{else}}
    import {
    {{pascalCase module}}
    } from '@o2s/framework/modules';
{{/if}}

export {
{{pascalCase module}}Service as Service } from './{{kebabCase module}}.service';

{{#if (eq module 'cms')}}
    export import Request = CMS.Request; export import Model = CMS.Model;
{{else}}
    export import Request =
    {{pascalCase module}}.Request; export import Model =
    {{pascalCase module}}.Model;
{{/if}}